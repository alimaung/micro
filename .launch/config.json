{
  "apps": {
    "http": {
      "servers": {
        "aero-labs": {
          "listen": [":80", ":443"],
          "routes": [
            {
            "match": [
              { "host": ["aero-labs.space", "www.aero-labs.space"] },
              { "path": ["/micro/demo*"] }
            ],
            "handle": [
              {
              "handler": "reverse_proxy",
              "upstreams": [{ "dial": "127.0.0.1:8000" }]
              }
            ]
            }
            
          ]
        }
      }
    }
  }
}
